# bc_desktop//submit/faculty.yml.erb
---
script:
  queue_name: <%= custom_queue %> 
  qos: <%= bc_qos %>
  email_on_terminated: <%= ! email_on_terminated.to_i.zero? %> <% unless email.blank? %>
  email: <%= email %> <% end %>

  native:
    - "-N"
    - "<%= bc_num_slots.blank? ? 1 : bc_num_slots.to_i %>"
    - "--job-name=ood-faculty" 
    - "--exclusive"
